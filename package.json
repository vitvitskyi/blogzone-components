{
  "name": "@livescorecom/react-components",
  "version": "1.0.0-beta.64",
  "description": "LiveScore React Components",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/livescorereactcomponents.esm.js",
  "unpkg": "dist/livescorereactcomponents.umd.production.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/minelytix/livescore-react-components.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch --format cjs,esm,umd --name livescoreReactComponents",
    "build": "tsdx build --format cjs,esm,umd --name livescoreReactComponents --transpileOnly",
    "test": "NODE_ICU_DATA=node_modules/full-icu tsdx test --passWithNoTests",
    "test:win": "set NODE_ICU_DATA=node_modules/full-icu && tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "lint:fix": "tsdx lint src --fix",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "deploy-storybook": "yarn build-storybook && storybook-to-ghpages --branch gh-pages --existing-output-dir storybook-static",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/react-modal": "^3.10.6",
    "js-cookie": "^2.2.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-hook-form": "^5.7.2",
    "react-modal": "^3.11.2",
    "react-share": "^4.2.0",
    "shortid": "^2.2.15"
  },
  "peerDependencies": {
    "@livescorecom/i18n": "^0.20.5",
    "@types/styled-components": ">=5",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5",
    "swr": "^0.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@livescorecom/i18n": "^0.20.5",
    "@rollup/plugin-image": "^2.0.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@testing-library/react": "^10.4.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react": "^16.9.38",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.8",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^5.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "full-icu": "^1.3.1",
    "husky": "^4.2.5",
    "jest-styled-components": "^7.0.2",
    "jest-svg-transformer": "^1.0.0",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "semantic-release": "^17.1.0",
    "styled-components": "^5.1.1",
    "swr": "^0.2.3",
    "ts-loader": "^7.0.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestsetup.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.ts"
    }
  }
}
